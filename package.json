{
  "name": "web-scraper",
  "version": "1.1.0",
  "main": "index.ts",
  "repository": "git@github.com:lkcozy/web-scraper.git",
  "author": "Kan Luo <lkklcozy@gmail.com>",
  "description": "A simple web scraping in TypeScript",
  "license": "MIT",
  "scripts": {
    "start": "tsc-watch --onSuccess \"yarn serve\"",
    "start:air-quality": "tsc-watch src/AirQuality/index.ts --outDir ./dist/AirQuality --onSuccess \"yarn serve:air-quality\"",
    "start:fang": "tsc-watch src/Fang/index.ts --outDir ./dist/Fang --onSuccess \"yarn serve:fang\"",
    "serve": "yarn node dist/index.js",
    "serve:air-quality": "yarn node dist/AirQuality/index.js",
    "serve:fang": "yarn node dist/Fang/index.js",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write --loglevel warn",
    "lint:all:format": "yarn format \"**/*.{ts,json,md}\"",
    "prepare": "husky install",
    "spell-check": "cspell --unique --no-progress \"**/*.{md,ts,tsx,js,css,less,json,yml,yaml,html,sh}\""
  },
  "dependencies": {
    "@actions/core": "^1.9.0",
    "await-to-js": "^3.0.0",
    "dotenv": "^16.0.0",
    "loglevel": "^1.7.1",
    "node-fetch": "2.6.1",
    "puppeteer": "^16.1.0",
    "ramda": "^0.28.0",
    "strip-bom": "^5.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/node-fetch": "^2.6.0",
    "@types/ramda": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cspell": "^6.6.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.7.0",
    "tsc-watch": "^5.0.0",
    "typescript": "^4.7.0"
  }
}
