{
  "name": "web-scraper",
  "version": "1.1.0",
  "main": "index.ts",
  "repository": "git@github.com:lkcozy/web-scraper.git",
  "author": "Kan Luo <lkklcozy@gmail.com>",
  "description": "A simple web scraping in TypeScript",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "watch": "tsc-watch",
    "prewatch": "yarn build",
    "start": "yarn watch --onSuccess \"yarn serve\"",
    "start:air-quality": "yarn watch src/AirQuality/index.ts --outDir ./dist/AirQuality --onSuccess \"yarn serve:air-quality\"",
    "start:fang": "yarn watch src/Fang/index.ts --outDir ./dist/Fang --onSuccess \"yarn serve:fang\"",
    "serve": "yarn node dist/index.js",
    "serve:air-quality": "yarn node dist/AirQuality/index.js",
    "serve:fang": "yarn node dist/Fang/index.js",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write --loglevel warn",
    "lint:all:format": "yarn format \"**/*.{ts,json,md}\"",
    "prepare": "husky install",
    "spell-check": "cspell --unique --no-progress \"**/*.{md,ts,tsx,js,css,less,json,yml,yaml,html,sh}\""
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "await-to-js": "^3.0.0",
    "dotenv": "^16.0.2",
    "loglevel": "^1.8.0",
    "node-fetch": "^3.3.0",
    "puppeteer": "^19.7.0",
    "ramda": "^0.28.0",
    "strip-bom": "^5.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^18.15.0",
    "@types/ramda": "^0.28.20",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cspell": "^6.28.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.0"
  },
  "engines": {
    "yarn": ">=1.22",
    "node": ">=16",
    "npm": "please-use-yarn"
  },
  "engineStrict": true
}
