{
  "name": "web-scraper",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:lkcozy/web-scraper.git",
  "author": "Kan Luo <lkklcozy@gmail.com>",
  "description": "A simple web scraping in TypeScript",
  "license": "MIT",
  "scripts": {
    "start": "tsc-watch --onSuccess \"yarn serve\"",
    "start:air-quality": "tsc-watch src/AirQuality/index.ts --outDir ./dist/AirQuality --onSuccess \"yarn serve:air-quality\"",
    "start:fang": "tsc-watch src/Fang/index.ts --outDir ./dist/Fang --onSuccess \"yarn serve:fang\"",
    "serve": "yarn node dist/index.js",
    "serve:air-quality": "yarn node dist/AirQuality/index.js",
    "serve:fang": "yarn node dist/Fang/index.js",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write --loglevel warn",
    "lint:all:format": "yarn format \"**/*.{ts,json,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@actions/core": "^1.4.0",
    "await-to-js": "^3.0.0",
    "dotenv": "^10.0.0",
    "loglevel": "^1.7.1",
    "node-fetch": "^2.6.1",
    "puppeteer": "^10.2.0",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/node-fetch": "^2.5.12",
    "@types/ramda": "^0.27.44",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.4.2"
  }
}
