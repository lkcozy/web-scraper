{
  "name": "web-scraper",
  "version": "2.0.0",
  "main": "index.ts",
  "repository": "git@github.com:lkcozy/web-scraper.git",
  "author": "lkcozy<lkklcozy@gmail.com>",
  "description": "A simple web scraping in TypeScript",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "watch": "tsc-watch",
    "prewatch": "yarn build",
    "start": "yarn watch --onSuccess \"yarn serve\"",
    "start:air-quality": "yarn watch src/AirQuality/index.ts --outDir ./dist/AirQuality --onSuccess \"yarn serve:air-quality\"",
    "start:fang": "yarn watch src/Fang/index.ts --outDir ./dist/Fang --onSuccess \"yarn serve:fang\"",
    "serve": "yarn node dist/index.js",
    "serve:air-quality": "yarn node dist/AirQuality/index.js",
    "serve:fang": "yarn node dist/Fang/index.js",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write --loglevel warn",
    "lint:all:format": "yarn format \"**/*.{ts,json,md}\"",
    "prepare": "husky install",
    "spell-check": "cspell --unique --no-progress \"**/*.{md,ts,tsx,js,css,less,json,yml,yaml,html,sh}\""
  },
  "dependencies": {
    "@actions/core": "^1",
    "await-to-js": "^3",
    "dotenv": "^16",
    "loglevel": "^1",
    "node-fetch": "^3",
    "puppeteer": "^21",
    "ramda": "^0",
    "strip-bom": "^5"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1",
    "@types/dotenv": "^8",
    "@types/node": "^20",
    "@types/ramda": "^0",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "cspell": "^7",
    "eslint": "^8",
    "eslint-config-airbnb": "^19",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-sonarjs": "^0",
    "husky": "^8",
    "lint-staged": "^13",
    "prettier": "^3",
    "tsc-watch": "^6",
    "typescript": "^5"
  },
  "engines": {
    "yarn": ">=1",
    "node": ">=16",
    "npm": "please-use-yarn"
  },
  "engineStrict": true
}
